<div class="row">
  <div class="small-4 columns">
    <%= image_tag(@user.profile_photo) %>
    <h4>@<%= @user.user_name %></h4>
  </div>
  <div class="small-8 columns">
    <h3><%= @entry.title %></h3>
    <h6><%= "#{@entry.date}, #{@entry.location.name}, #{@entry.country.name}, #{@entry.region.name}"%></h6>
    Swell data
  </div>
</div>
<div class="row">
  <div class="small-6 columns">
    Photos
    Map
    <%= form_for [@entry, @photo] do |f| %>
      <%= f.label :file_name, "Profile Photo" %>
      <%= f.file_field :file_name %><br />
      <%= f.submit "Add Photo", class: "button" %>
    <% end %>
    <% @photos.each do |photo| %>
      <%= image_tag(photo) %>
    <% end %>
  </div>
  <div class="small-6 columns">

    <p><%= @entry.body %></p>
  </div>
</div>
