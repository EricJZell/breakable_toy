<div class="row">
  <div class="small-8 small-centered columns">
    <h3 class="title text-center div-bar">Search Users</h3>
    <ul class="content">
      <% @users.each do |user| %>
        <li class="entries">
            <%= image_tag(user.profile_photo, id:"mini-thumb", class:"user-index" ) %>
            <h4 class="user-index"><%= link_to "@#{user.user_name}", user%></h4>
            <h6 class="user-index">(<%=user.full_name%>)</h6>
        </li>
      <% end %>
    </ul>
  </div>
</div>
